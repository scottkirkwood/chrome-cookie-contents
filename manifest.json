{
  "name": "ChromeCookieContents",
  "version": "1.0",
  "description": "Show the cookies for this page in text format.",
  "icons": {
    "16": "icon-16.png",
    "32": "icon-32.png",
    "48": "icon-48.png",
    "128": "icon-128.png"
  },
  "permissions": [
    "cookies",
    "tabs",
    "http://*/*", "https://*/*"
  ],
  "background_page": "background.html",

  // Inject jquery
  "browser_action": {
    "default_icon": "icon-19.png",
    "default_title": "CookieJar"
  }
}
